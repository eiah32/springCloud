<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<title>用户列表</title>
</head>
<body>
	<table id="dg" title="My Users" class="easyui-datagrid" style="width:700px;height:250px"
            url="/user/showAllUsers"
            toolbar="#toolbar" pagination="true"
            rownumbers="true" fitColumns="true" singleSelect="true">
        <thead>
            <tr>
                <th field="id" width="50" height="30">id</th>
                <th field="username" width="50" height="30">username</th>
                <th field="password" width="50" height="30">Phone</th>
                <th field="gender" width="50" height="30">Email</th>
                <th field="email" width="50" height="30">Email</th>
                <th field="tel" width="50" height="30">Email</th>
                <th field="registDate" width="50" height="30">Email</th>
                <th field="lastLoginDate" width="50" height="30">Email</th>
                <th field="status" width="50" height="30">Email</th>
                <th field="salt" width="50" height="30">Email</th>
                <th field="credentialsSalt" width="50" height="30">Email</th>
            </tr>
        </thead>
    </table>
    <div id="toolbar">
        <a href="javascript:void(0)" class="easyui-linkbutton" iconCls="icon-add" plain="true" onclick="newUser()">New User</a>
        <a href="javascript:void(0)" class="easyui-linkbutton" iconCls="icon-edit" plain="true" onclick="editUser()">Edit User</a>
        <a href="javascript:void(0)" class="easyui-linkbutton" iconCls="icon-remove" plain="true" onclick="destroyUser()">Remove User</a>
    </div>
